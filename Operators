a = 3
b = 4

# Arithmetic Operators
print("The value of a + b is " , a + b)
print("The value of a - b is " , a - b)
print("The value of a * b is " , a * b)
print("The value of a / b is " , a / b)

# Assignment Operators
a = 3
a += 2
print("The updated value of a is ", a)

# Comparison Operators
a = 2
b = 2

if(a>=b):
    print(True)
else:
    print(False)


# Logical Operators
bool1 = True
bool2 = False

print("The value of bool1 and bool2 is ", (bool1 and bool2))
