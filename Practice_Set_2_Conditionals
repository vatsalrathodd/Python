sub1 = int(input("Enter marks of subject 1: "))
sub2 = int(input("Enter marks of subject 2: "))
sub3 = int(input("Enter marks of subject 3: "))

sub1_pr = (sub1 * 33) / 100
sub2_pr = (sub2 * 33) / 100
sub3_pr = (sub3 * 33) / 100


total = sub1 + sub2 + sub3 
pr = (total * 40) / 100

if(pr>40 and sub1_pr>33 and sub2_pr>33 and sub3_pr>33):
    print("PASS!!")
else:
    print("FAIL...")
